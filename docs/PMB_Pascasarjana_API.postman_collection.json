            "method": "GET",
            "header": [
              {
                "key": "Accept",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{base_url}}/prodi",
              "host": ["{{base_url}}"],
              "path": ["prodi"]
            },
            "description": "Mendapatkan daftar program studi yang aktif untuk form registrasi"
          },
          "response": []
        },
        {
          "name": "Register Pendaftar",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "if (jsonData.success && jsonData.data.kode_akses) {",
                  "    pm.collectionVariables.set('kode_akses', jsonData.data.kode_akses);",
                  "    pm.collectionVariables.set('nomor_pendaftaran', jsonData.data.nomor_pendaftaran);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Accept",
                "value": "application/json"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"nama_lengkap\": \"John Doe\",\n    \"no_whatsapp\": \"081234567890\",\n    \"prodi_id\": 1\n}"
            },
            "url": {
              "raw": "{{base_url}}/register",
              "host": ["{{base_url}}"],
              "path": ["register"]
            },
            "description": "Registrasi pendaftar baru. Akan mengembalikan nomor pendaftaran dan kode akses."
